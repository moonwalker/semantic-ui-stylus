/*******************************
            Theme
*******************************/

$ui_type    = 'collection';
$ui_element = 'table';

@import '../../theme.config';

/*******************************
             Table
*******************************/

/* Prototype */
.ui.table {
  width: 100%;
  background: $ui_background;
  margin: $ui_margin;
  border: $ui_border;
  box-shadow: $ui_boxShadow;
  border-radius: $ui_borderRadius;
  text-align: $ui_textAlign;
  color: $ui_color;
  border-collapse: $ui_borderCollapse;
  border-spacing: $ui_borderSpacing;
}

.ui.table:first-child {
  margin-top: 0em;
}
.ui.table:last-child {
  margin-bottom: 0em;
}

/*******************************
             Parts
*******************************/

/* Table Content */
.ui.table th,
.ui.table td {
  transition: $ui_transition;
}

/* Headers */
.ui.table thead {
  box-shadow: $ui_headerBoxShadow;
}
.ui.table thead th {
  cursor: auto;
  background: $ui_headerBackground;
  text-align: $ui_headerAlign;
  color: $ui_headerColor;
  padding: $ui_headerVerticalPadding $ui_headerHorizontalPadding;
  vertical-align: $ui_headerVerticalAlign;
  font-style: $ui_headerFontStyle;
  font-weight: $ui_headerFontWeight;
  text-transform: $ui_headerTextTransform;
  border-bottom: $ui_headerBorder;
  border-left: $ui_headerDivider;
}

.ui.table thead tr > th:first-child {
  border-left: none;
}

.ui.table thead tr:first-child > th:first-child {
  border-radius: $ui_borderRadius 0em 0em 0em;
}
.ui.table thead tr:first-child > th:last-child {
  border-radius: 0em $ui_borderRadius 0em 0em;
}
.ui.table thead tr:first-child > th:only-child {
  border-radius: $ui_borderRadius $ui_borderRadius 0em 0em;
}

/* Footer */
.ui.table tfoot {
  box-shadow: $ui_footerBoxShadow;
}
.ui.table tfoot th {
  cursor: auto;
  border-top: $ui_footerBorder;
  background: $ui_footerBackground;
  text-align: $ui_footerAlign;
  color: $ui_footerColor;
  padding: $ui_footerVerticalPadding $ui_footerHorizontalPadding;
  vertical-align: $ui_footerVerticalAlign;
  font-style: $ui_footerFontStyle;
  font-weight: $ui_footerFontWeight;
  text-transform: $ui_footerTextTransform;
}
.ui.table tfoot tr > th:first-child {
  border-left: none;
}
.ui.table tfoot tr:first-child > th:first-child {
  border-radius: 0em 0em 0em $ui_borderRadius;
}
.ui.table tfoot tr:first-child > th:last-child {
  border-radius: 0em 0em $ui_borderRadius 0em;
}
.ui.table tfoot tr:first-child > th:only-child {
  border-radius: 0em 0em $ui_borderRadius $ui_borderRadius;
}

/* Table Row */
.ui.table tr td {
  border-top: $ui_rowBorder;
}
.ui.table tr:first-child td {
  border-top: none;
}

/* Table Cells */
.ui.table td {
  padding: $ui_cellVerticalPadding $ui_cellHorizontalPadding;
  text-align: $ui_cellTextAlign;
}

/* Icons */
.ui.table > .icon {
  vertical-align: $ui_iconVerticalAlign;
}
.ui.table > .icon:only-child {
  margin: 0em;
}

/* Table Segment */
.ui.table.segment {
  padding: 0em;
}
.ui.table.segment:after {
  display: none;
}
.ui.table.segment.stacked:after {
  display: block;
}


/* Responsive */
@media only screen and (max-width : $ui_largestMobileScreen) {
  .ui.table:not(.unstackable) {
    width: 100%;
  }
  .ui.table:not(.unstackable) tbody,
  .ui.table:not(.unstackable) tr,
  .ui.table:not(.unstackable) tr > th,
  .ui.table:not(.unstackable) tr > td  {
    display: block !important;
    width: auto !important;
    display: block !important;
  }

  .ui.table:not(.unstackable) {
    padding: 0em;
  }
  .ui.table:not(.unstackable) thead {
    display: $ui_responsiveHeaderDisplay;
  }
  .ui.table:not(.unstackable) tfoot {
    display: $ui_responsiveFooterDisplay;
  }
  .ui.table:not(.unstackable) tr {
    padding-top: $ui_responsiveRowVerticalPadding;
    padding-bottom: $ui_responsiveRowVerticalPadding;
    box-shadow: $ui_responsiveRowBoxShadow;
  }

  .ui.table:not(.unstackable) tr > th,
  .ui.table:not(.unstackable) tr > td {
    background: none;
    border: none !important;
    padding: $ui_responsiveCellVerticalPadding $ui_responsiveCellHorizontalPadding !important;
    box-shadow: $ui_responsiveCellBoxShadow;
  }
  .ui.table:not(.unstackable) th:first-child,
  .ui.table:not(.unstackable) td:first-child {
    font-weight: bold;
  }

  /* Definition Table */
  .ui.definition.table:not(.unstackable) thead th:first-child {
    box-shadow: none !important;
  }
}


/*******************************
            Coupling
*******************************/

/* UI Image */
.ui.table th .image,
.ui.table th .image img,
.ui.table td .image,
.ui.table td .image img {
  max-width: none;
}


/*******************************
             Types
*******************************/

/*--------------
    Complex
---------------*/

.ui.structured.table {
  border-collapse: collapse;
}
.ui.structured.table thead th {
  border-left: $ui_headerDivider;
  border-right: $ui_headerDivider;
}
.ui.structured.sortable.table thead th {
  border-left: $ui_sortableBorder;
  border-right: $ui_sortableBorder;
}
.ui.structured.basic.table th {
  border-left: $ui_basicTableHeaderDivider;
  border-right: $ui_basicTableHeaderDivider;
}
.ui.structured.celled.table tr th,
.ui.structured.celled.table tr td {
  border-left: $ui_cellBorder;
  border-right: $ui_cellBorder;
}

/*--------------
   Definition
---------------*/

.ui.definition.table thead:not(.full-width) th:first-child {
  pointer-events: none;
  background: $ui_definitionHeaderBackground;
  font-weight: $ui_definitionHeaderFontWeight;
  color: $ui_definitionHeaderColor;
  box-shadow: - $ui_borderWidth - $ui_borderWidth 0px $ui_borderWidth $ui_definitionPageBackground;
}

.ui.definition.table tfoot:not(.full-width) th:first-child {
  pointer-events: none;
  background: $ui_definitionFooterBackground;
  font-weight: $ui_definitionFooterColor;
  color: $ui_definitionFooterFontWeight;
  box-shadow: $ui_borderWidth $ui_borderWidth 0px $ui_borderWidth $ui_definitionPageBackground;
}

/* Remove Border */
.ui.celled.definition.table thead:not(.full-width) th:first-child {
  box-shadow: 0px (- $ui_borderWidth) 0px $ui_borderWidth $ui_definitionPageBackground;
}
.ui.celled.definition.table tfoot:not(.full-width) th:first-child {
  box-shadow: 0px $ui_borderWidth 0px $ui_borderWidth $ui_definitionPageBackground;
}

/* Highlight Defining Column */
.ui.definition.table tr td:first-child:not(.ignored),
.ui.definition.table tr td.definition {
  background: $ui_definitionColumnBackground;
  font-weight: $ui_definitionColumnFontWeight;
  color: $ui_definitionColumnColor;
  text-transform: $ui_definitionColumnTextTransform;
  box-shadow: $ui_definitionColumnBoxShadow;
  text-align: $ui_definitionColumnTextAlign;
  font-size: $ui_definitionColumnFontSize;
  padding-left: $ui_definitionColumnHorizontalPadding;
  padding-right: $ui_definitionColumnHorizontalPadding;
}


/* Fix 2nd Column */
.ui.definition.table thead:not(.full-width) th:nth-child(2) {
  border-left: $ui_borderWidth solid $ui_borderColor;
}
.ui.definition.table tfoot:not(.full-width) th:nth-child(2) {
  border-left: $ui_borderWidth solid $ui_borderColor;
}
.ui.definition.table td:nth-child(2) {
