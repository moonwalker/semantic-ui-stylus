.ui.sortable.table thead th.sorted {
  background: $ui_sortableActiveBackground;
  color: $ui_sortableActiveColor;
}
.ui.sortable.table thead th.sorted:after {
  display: inline-block;
}

/* Sorted Hover */
.ui.sortable.table thead th.sorted:hover {
  background: $ui_sortableActiveHoverBackground;
  color: $ui_sortableActiveHoverColor;
}

/* Inverted */
.ui.inverted.sortable.table thead th.sorted {
  background: $ui_sortableInvertedActiveBackground;
  color: $ui_sortableInvertedActiveColor;
}
.ui.inverted.sortable.table thead th:hover {
  background: $ui_sortableInvertedHoverBackground;
  color: $ui_sortableInvertedHoverColor;
}
.ui.inverted.sortable.table thead th {
  border-left-color: $ui_sortableInvertedBorderColor;
  border-right-color: $ui_sortableInvertedBorderColor;
}


/*--------------
    Inverted
---------------*/

/* Text Color */
.ui.inverted.table {
  background: $ui_invertedBackground;
  color: $ui_invertedCellColor;
  border: $ui_invertedBorder;
}
.ui.inverted.table th {
  background-color: $ui_invertedHeaderBackground;
  border-color: $ui_invertedHeaderBorderColor !important;
  color: $ui_invertedHeaderColor;
}
.ui.inverted.table tr td {
  border-color: $ui_invertedCellBorderColor !important;
}

.ui.inverted.table tr.disabled td,
.ui.inverted.table tr td.disabled,
.ui.inverted.table tr.disabled:hover td,
.ui.inverted.table tr:hover td.disabled {
  pointer-events: none;
  color: $ui_invertedDisabledTextColor;
}

/* Definition */
.ui.inverted.definition.table tfoot:not(.full-width) th:first-child,
.ui.inverted.definition.table thead:not(.full-width) th:first-child {
  background: $ui_definitionPageBackground;
}
.ui.inverted.definition.table tr td:first-child {
  background: $ui_invertedDefinitionColumnBackground;
  color: $ui_invertedDefinitionColumnColor;
}

/*--------------
   Collapsing
---------------*/

.ui.collapsing.table {
  width: auto;
}

/*--------------
      Basic
---------------*/

.ui.basic.table {
  background: $ui_basicTableBackground;
  border: $ui_basicTableBorder;
  box-shadow: $ui_basicBoxShadow;
}
.ui.basic.table thead,
.ui.basic.table tfoot {
  box-shadow: none;
}
.ui.basic.table th {
  background: $ui_basicTableHeaderBackground;
  border-left: $ui_basicTableHeaderDivider;
}
.ui.basic.table tbody tr {
  border-bottom: $ui_basicTableCellBorder;
}
.ui.basic.table td {
  background: $ui_basicTableCellBackground;
}
.ui.basic.striped.table tbody tr:nth-child(2n) {
  background-color: $ui_basicTableStripedBackground !important;
}

/* Very Basic */
.ui[class*="very basic"].table {
  border: none;
}
.ui[class*="very basic"].table:not(.sortable):not(.striped) th,
.ui[class*="very basic"].table:not(.sortable):not(.striped) td {
  padding: $ui_basicTableCellPadding;
}
.ui[class*="very basic"].table:not(.sortable):not(.striped) th:first-child,
.ui[class*="very basic"].table:not(.sortable):not(.striped) td:first-child {
  padding-left: 0em;
}
.ui[class*="very basic"].table:not(.sortable):not(.striped) th:last-child,
.ui[class*="very basic"].table:not(.sortable):not(.striped) td:last-child {
  padding-right: 0em;
}
.ui[class*="very basic"].table:not(.sortable):not(.striped) thead tr:first-child th {
  padding-top: 0em;
}

/*--------------
     Celled
---------------*/

.ui.celled.table tr th,
.ui.celled.table tr td {
  border-left: $ui_cellBorder;
}
.ui.celled.table tr th:first-child,
.ui.celled.table tr td:first-child {
  border-left: none;
}

/*--------------
     Padded
---------------*/

.ui.padded.table th {
  padding-left: $ui_paddedHorizontalPadding;
  padding-right: $ui_paddedHorizontalPadding;
}
.ui.padded.table th,
.ui.padded.table td {
  padding: $ui_paddedVerticalPadding $ui_paddedHorizontalPadding;
}

/* Very */
.ui[class*="very padded"].table th {
  padding-left: $ui_veryPaddedHorizontalPadding;
  padding-right: $ui_veryPaddedHorizontalPadding;
}
.ui[class*="very padded"].table td {
  padding: $ui_veryPaddedVerticalPadding $ui_veryPaddedHorizontalPadding;
}

/*--------------
     Compact
---------------*/

.ui.compact.table th {
  padding-left: $ui_compactHorizontalPadding;
  padding-right: $ui_compactHorizontalPadding;
}
.ui.compact.table td {
  padding: $ui_compactVerticalPadding $ui_compactHorizontalPadding;
}

/* Very */
.ui[class*="very compact"].table th {
  padding-left: $ui_veryCompactHorizontalPadding;
  padding-right: $ui_veryCompactHorizontalPadding;
}
.ui[class*="very compact"].table td {
  padding: $ui_veryCompactVerticalPadding $ui_veryCompactHorizontalPadding;
}

/*--------------
      Sizes
---------------*/

/* Small */
.ui.small.table {
  font-size: $ui_small;
}

/* Standard */
.ui.table {
  font-size: $ui_medium;
}

/* Large */
.ui.large.table {
  font-size: $ui_large;
}

loadUIOverrides();

