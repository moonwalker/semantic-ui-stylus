/*******************************
            Theme
*******************************/

$ui_type    = 'element';
$ui_element = 'button';

@import '../../theme.config';

/*******************************
            Button
*******************************/

.ui.button {
  cursor: pointer;
  display: inline-block;

  min-height: 1em;

  outline: none;
  border: none;
  vertical-align: $ui_verticalAlign;
  background: $ui_background;
  color: $ui_textColor;

  font-family: $ui_fontFamily;

  margin: 0em $ui_horizontalMargin $ui_verticalMargin 0em;
  padding: $ui_verticalPadding $ui_horizontalPadding ($ui_verticalPadding + $ui_shadowOffset);

  text-transform: $ui_textTransform;
  text-shadow: $ui_textShadow;
  font-weight: $ui_fontWeight;
  line-height: $ui_lineHeight;
  font-style: normal;
  text-align: center;
  text-decoration: none;

  border-radius: $ui_borderRadius;
  box-shadow: $ui_boxShadow;

  user-select: none;
  transition: $ui_transition;
  will-change: $ui_willChange;

  -webkit-tap-highlight-color: $ui_tapColor;
}


/*******************************
            States
*******************************/

/*--------------
      Hover
---------------*/

.ui.button:hover {
  background-color: $ui_hoverBackgroundColor;
  background-image: $ui_hoverBackgroundImage;
  box-shadow: $ui_hoverBoxShadow;
  color: $ui_hoverColor;
}

.ui.button:hover .icon {
  opacity: $ui_iconHoverOpacity;
}

/*--------------
      Focus
---------------*/

.ui.button:focus {
  background-color: $ui_focusBackgroundColor;
  color: $ui_focusColor;
  background-image: $ui_focusBackgroundImage !important;
  box-shadow: $ui_focusBoxShadow !important;
}

.ui.button:focus .icon {
  opacity: $ui_iconFocusOpacity;
}

/*--------------
      Down
---------------*/

.ui.button:active,
.ui.active.button:active {
  background-color: $ui_downBackgroundColor;
  background-image: $ui_downBackgroundImage;
  color: $ui_downColor;
  box-shadow: $ui_downBoxShadow;
}

/*--------------
     Active
---------------*/

.ui.active.button {
  background-color: $ui_activeBackgroundColor;
  background-image: $ui_activeBackgroundImage;
  box-shadow: $ui_activeBoxShadow;
  color: $ui_activeColor;
}
.ui.active.button:hover {
  background-color: $ui_activeHoverBackgroundColor;
  background-image: $ui_activeHoverBackgroundImage;
  color: $ui_activeHoverColor;
}
.ui.active.button:active {
  background-color: $ui_activeBackgroundColor;
  background-image: $ui_activeBackgroundImage;
}


/*--------------
    Loading
---------------*/

/* Specificity hack */
.ui.loading.loading.loading.loading.loading.loading.button {
  position: relative;
  cursor: default;
  text-shadow: none !important;
  color: transparent !important;
  opacity: $ui_loadingOpacity;
  pointer-events: $ui_loadingPointerEvents;
  transition: $ui_loadingTransition;
}
.ui.loading.button:before {
  position: absolute;
  content: '';
  top: 50%;
  left: 50%;

  margin: $ui_loaderMargin;
  width: $ui_loaderSize;
  height: $ui_loaderSize;

  border-radius: $ui_circularRadius;
  border: $ui_loaderLineWidth solid $ui_invertedLoaderFillColor;
}
.ui.loading.button:after {
  position: absolute;
  content: '';
  top: 50%;
  left: 50%;

  margin: $ui_loaderMargin;
  width: $ui_loaderSize;
  height: $ui_loaderSize;

  animation: button-spin $ui_loaderSpeed linear;
  animation-iteration-count: infinite;

  border-radius: $ui_circularRadius;

  border-color: $ui_invertedLoaderLineColor transparent transparent;
  border-style: solid;
  border-width: $ui_loaderLineWidth;

  box-shadow: 0px 0px 0px 1px transparent;
}
.ui.labeled.icon.loading.button .icon {
  background-color: transparent;
  box-shadow: none;
}

@keyframes button-spin {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}

.ui.basic.loading.button:not(.inverted):before {
  border-color: $ui_loaderFillColor;
}
.ui.basic.loading.button:not(.inverted):after {
  border-top-color: $ui_loaderLineColor;
}

/*-------------------
      Disabled
--------------------*/

.ui.buttons .disabled.button,
.ui.disabled.button,
.ui.button:disabled,
.ui.disabled.button:hover,
.ui.disabled.active.button {
  cursor: default;
  opacity: $ui_disabledOpacity !important;
  background-image: none !important;
  box-shadow: none !important;
  pointer-events: none !important;
}

/* Basic Group With Disabled */
.ui.basic.buttons .ui.disabled.button {
  border-color: $ui_disabledBorderColor;
}

/*******************************
             Types
*******************************/

/*-------------------
       Animated
--------------------*/

.ui.animated.button {
  position: relative;
  overflow: hidden;
  padding-right: 0em !important;
  vertical-align: $ui_animatedVerticalAlign;
  z-index: $ui_animatedZIndex;
}

.ui.animated.button .content {
  will-change: transform, opacity;
}
.ui.animated.button .visible.content {
  position: relative;
  margin-right: $ui_horizontalPadding;
}
.ui.animated.button .hidden.content {
  position: absolute;
  width: 100%;
}

/* Horizontal */
.ui.animated.button .visible.content,
.ui.animated.button .hidden.content {
  transition: right $ui_animationDuration $ui_animationEasing 0s;
}
.ui.animated.button .visible.content {
  left: auto;
  right: 0%;
}
.ui.animated.button .hidden.content {
  top: 50%;
  left: auto;
  right: -100%;
  margin-top: -($ui_lineHeight / 2);
}
.ui.animated.button:focus .visible.content,
.ui.animated.button:hover .visible.content {
  left: auto;
  right: 200%;
}
.ui.animated.button:focus .hidden.content,
.ui.animated.button:hover .hidden.content {
  left: auto;
  right: 0%;
}

/* Vertical */
.ui.vertical.animated.button .visible.content,
.ui.vertical.animated.button .hidden.content {
  transition: top $ui_animationDuration $ui_animationEasing, transform $ui_animationDuration $ui_animationEasing;
}
.ui.vertical.animated.button .visible.content {
  transform: translateY(0%);
  right: auto;
}
.ui.vertical.animated.button .hidden.content {
  top: -50%;
  left: 0%;
  right: auto;
}
.ui.vertical.animated.button:focus .visible.content,
.ui.vertical.animated.button:hover .visible.content {
  transform: translateY(200%);
  right: auto;
}
.ui.vertical.animated.button:focus .hidden.content,
.ui.vertical.animated.button:hover .hidden.content {
  top: 50%;
  right: auto;
}

/* Fade */
.ui.fade.animated.button .visible.content,
.ui.fade.animated.button .hidden.content {
  transition: opacity $ui_animationDuration $ui_animationEasing, transform $ui_animationDuration $ui_animationEasing;
}
.ui.fade.animated.button .visible.content {
  left: auto;
  right: auto;
  opacity: 1;
  transform: scale(1);
}
.ui.fade.animated.button .hidden.content {
  opacity: 0;
  left: 0%;
  right: auto;
  transform: scale($ui_fadeScaleHigh);
}
.ui.fade.animated.button:focus .visible.content,
.ui.fade.animated.button:hover .visible.content {
  left: auto;
  right: auto;
  opacity: 0;
  transform: scale($ui_fadeScaleLow);
}
.ui.fade.animated.button:focus .hidden.content,
.ui.fade.animated.button:hover .hidden.content {
  left: 0%;
  right: auto;
  opacity: 1;
  transform: scale(1);
}

/*-------------------
       Inverted
--------------------*/

.ui.inverted.button {
  box-shadow: 0px 0px 0px $ui_invertedBorderSize $ui_white inset !important;
  background: transparent none;
  color: $ui_white;
  text-shadow: none !important;
}

/* Group */
.ui.inverted.buttons .button {
  margin: $ui_invertedGroupButtonOffset;
}
.ui.inverted.buttons .button:first-child {
  margin-left: 0em;
}
.ui.inverted.vertical.buttons .button {
  margin: $ui_invertedVerticalGroupButtonOffset;
}
.ui.inverted.vertical.buttons .button:first-child {
  margin-top: 0em;
}

/* States */

/* Hover */
.ui.inverted.button:hover {
  background: $ui_white;
  box-shadow: 0px 0px 0px $ui_invertedBorderSize $ui_white inset !important;
  color: $ui_hoverColor;
}

/* Active / Focus */
.ui.inverted.button:focus,
.ui.inverted.button.active {
  background: $ui_white;
  box-shadow: 0px 0px 0px $ui_invertedBorderSize $ui_white inset !important;
  color: $ui_focusColor;
}

/* Active Focus */
.ui.inverted.button.active:focus {
  background: $ui_midWhite;
  box-shadow: 0px 0px 0px $ui_invertedBorderSize $ui_midWhite inset !important;
  color: $ui_focusColor;
}


/*-------------------
    Labeled Button
--------------------*/

.ui.labeled.button:not(.icon) {
  display: inline-flex;
  flex-direction: row;
  background: none !important;
  padding: 0px !important;
  border: none !important;
  box-shadow: none !important;
}

.ui.labeled.button > .button {
  margin: 0px;
}
.ui.labeled.button > .label {
  display: flex;
  align-items: $ui_labeledLabelAlign;
  margin: 0px 0px 0px $ui_labeledLabelBorderOffset !important;
  font-size: $ui_labeledLabelFontSize;
  padding: $ui_labeledLabelPadding;
  font-size: $ui_labeledLabelFontSize;
  border-color: $ui_labeledLabelBorderColor;
}

/* Tag */
.ui.labeled.button > .tag.label:before {
  width: $ui_labeledTagLabelSize;
  height: $ui_labeledTagLabelSize;
}

/* Right */
.ui.labeled.button:not([class*="left labeled"]) > .button {
  border-top-right-radius: 0px;
  border-bottom-right-radius: 0px;
}
.ui.labeled.button:not([class*="left labeled"]) > .label {
  border-top-left-radius: 0px;
  border-bottom-left-radius: 0px;
}

/* Left Side */
.ui[class*="left labeled"].button > .button {
  border-top-left-radius: 0px;
  border-bottom-left-radius: 0px;
}
.ui[class*="left labeled"].button > .label {
  border-top-right-radius: 0px;
  border-bottom-right-radius: 0px;
}

