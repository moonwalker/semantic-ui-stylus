  transition: $ui_iconTransition;
  vertical-align: $ui_iconVerticalAlign;
  color: $ui_iconColor;
}

.ui.button:not(.icon) > .icon:not(.button):not(.dropdown) {
  margin: $ui_iconMargin;
}
.ui.button:not(.icon) > .right.icon:not(.button):not(.dropdown) {
  margin: $ui_rightIconMargin;
}

/*******************************
           Variations
*******************************/


/*-------------------
       Floated
--------------------*/

.ui[class*="left floated"].buttons,
.ui[class*="left floated"].button {
  float: left;
  margin-left: 0em;
  margin-right: $ui_floatedMargin;
}
.ui[class*="right floated"].buttons,
.ui[class*="right floated"].button {
  float: right;
  margin-right: 0em;
  margin-left: $ui_floatedMargin;
}

/*-------------------
       Compact
--------------------*/

.ui.compact.buttons .button,
.ui.compact.button {
  padding: $ui_compactVerticalPadding $ui_compactHorizontalPadding ( $ui_compactVerticalPadding + $ui_shadowOffset );
}
.ui.compact.icon.buttons .button,
.ui.compact.icon.button {
  padding: $ui_compactVerticalPadding $ui_compactVerticalPadding ( $ui_compactVerticalPadding + $ui_shadowOffset );
}
.ui.compact.labeled.icon.buttons .button,
.ui.compact.labeled.icon.button {
  padding: $ui_compactVerticalPadding ($ui_compactHorizontalPadding + $ui_labeledIconWidth) ( $ui_compactVerticalPadding + $ui_shadowOffset );
}

