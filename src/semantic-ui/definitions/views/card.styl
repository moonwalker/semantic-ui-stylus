.ui.link.raised.card:hover {
  box-shadow: $ui_raisedShadowHover;
}

.ui.raised.cards > .card,
.ui.raised.card {
  box-shadow: $ui_raisedShadow;
}
/*-------------------
       Centered
--------------------*/

.ui.centered.cards {
  justify-content: center;
}
.ui.centered.card {
  margin-left: auto;
  margin-right: auto;
}

/*-------------------
        Fluid
--------------------*/

.ui.fluid.card {
  width: 100%;
  max-width: 9999px;
}

/*-------------------
        Link
--------------------*/

.ui.cards a.card,
.ui.link.cards .card,
a.ui.card,
.ui.link.card {
  transform: none;
}


.ui.cards a.card:hover,
.ui.link.cards .card:hover,
a.ui.card:hover,
.ui.link.card:hover {
  cursor: pointer;
  z-index: $ui_linkHoverZIndex;
  background: $ui_linkHoverBackground;
  border: $ui_linkHoverBorder;
  box-shadow: $ui_linkHoverBoxShadow;
  transform: $ui_linkHoverTransform;
}

