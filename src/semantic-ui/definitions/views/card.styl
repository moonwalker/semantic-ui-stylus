  color: $ui_likeColor;
}
.ui.cards > .card > .content .active.like.icon,
.ui.card > .content .active.like.icon {
  color: $ui_likeActiveColor;
}

/*----------------
  Extra Content
-----------------*/

.ui.cards > .card > .extra,
.ui.card > .extra {
  max-width: 100%;
  min-height: 0em !important;
  flex-grow: 0;
  border-top: $ui_extraDivider !important;
  position: $ui_extraPosition;
  background: $ui_extraBackground;
  width: $ui_extraWidth;
  margin: $ui_extraMargin;
  padding: $ui_extraPadding;
  top: $ui_extraTop;
  left: $ui_extraLeft;
  color: $ui_extraColor;
  box-shadow: $ui_extraBoxShadow;
  transition: $ui_extraTransition;
}
.ui.cards > .card > .extra a:not(.ui),
.ui.card > .extra a:not(.ui) {
  color: $ui_extraLinkColor;
}
.ui.cards > .card > .extra a:not(.ui):hover,
.ui.card > .extra a:not(.ui):hover {
  color: $ui_extraLinkHoverColor;
}


