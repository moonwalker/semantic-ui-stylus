/*-------------------
       Em Sizes
--------------------*/

/*
  This rounds $ui_size values to the closest pixel then expresses that value in (r)em.
  This ensures all size values round to exact pixels
*/
$ui_mini            = unit( round($ui_miniSize * $ui_emSize) / $ui_emSize, rem);
$ui_tiny            = unit( round($ui_tinySize * $ui_emSize) / $ui_emSize, rem);
$ui_small           = unit( round($ui_smallSize * $ui_emSize) / $ui_emSize, rem);
$ui_medium          = unit( round($ui_mediumSize * $ui_emSize) / $ui_emSize, rem);
$ui_large           = unit( round($ui_largeSize * $ui_emSize) / $ui_emSize, rem);
$ui_big             = unit( round($ui_bigSize * $ui_emSize) / $ui_emSize, rem);
$ui_huge            = unit( round($ui_hugeSize * $ui_emSize) / $ui_emSize, rem);
$ui_massive         = unit( round($ui_massiveSize * $ui_emSize) / $ui_emSize, rem);

/* em */
$ui_relativeMini    = unit( round($ui_miniSize * $ui_emSize) / $ui_emSize, em);
$ui_relativeTiny    = unit( round($ui_tinySize * $ui_emSize) / $ui_emSize, em);
$ui_relativeSmall   = unit( round($ui_smallSize * $ui_emSize) / $ui_emSize, em);
$ui_relativeMedium  = unit( round($ui_mediumSize * $ui_emSize) / $ui_emSize, em);
$ui_relativeLarge   = unit( round($ui_largeSize * $ui_emSize) / $ui_emSize, em);
$ui_relativeBig     = unit( round($ui_bigSize * $ui_emSize) / $ui_emSize, em);
$ui_relativeHuge    = unit( round($ui_hugeSize * $ui_emSize) / $ui_emSize, em);
$ui_relativeMassive = unit( round($ui_massiveSize * $ui_emSize) / $ui_emSize, em);

/* rem */
$ui_absoluteMini    = unit( round($ui_miniSize * $ui_emSize) / $ui_emSize, rem);
$ui_absoluteTiny    = unit( round($ui_tinySize * $ui_emSize) / $ui_emSize, rem);
$ui_absoluteSmall   = unit( round($ui_smallSize * $ui_emSize) / $ui_emSize, rem);
