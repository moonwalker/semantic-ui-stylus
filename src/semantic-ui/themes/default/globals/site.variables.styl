/*******************************
         Site Settings
*******************************/

/*-------------------
       Fonts
--------------------*/

$ui_fontName          = 'Lato';
$ui_fontSmoothing     = antialiased;

$ui_headerFont        = $ui_fontName, 'Helvetica Neue', Arial, Helvetica, sans-serif;
$ui_pageFont          = $ui_fontName, 'Helvetica Neue', Arial, Helvetica, sans-serif;

$ui_googleFontName    = $ui_fontName;
$ui_importGoogleFonts = true;
$ui_googleFontSizes   = '400,700,400italic,700italic';
$ui_googleSubset      = 'latin';

$ui_googleProtocol    = 'https://';
$ui_googleFontRequest = '' + $ui_googleFontName + ':' + $ui_googleFontSizes + '&subset=' + $ui_googleSubset + '';

/*-------------------
      Base Sizes
--------------------*/

/* This is the single variable that controls them all */
$ui_emSize   = 14px;

/* The size of page text  */
$ui_fontSize = 14px;


/*-------------------
    Border Radius
--------------------*/

/* See Power-user section below
   for explanation of $ui_px variables
*/
$ui_relativeBorderRadius= $ui_relative4px;
$ui_absoluteBorderRadius= $ui_4px;

$ui_defaultBorderRadius= $ui_absoluteBorderRadius;

/*-------------------
    Brand Colors
--------------------*/

$ui_primaryColor        = $ui_blue;
$ui_secondaryColor      = $ui_black;

$ui_lightPrimaryColor   = $ui_lightBlue;
$ui_lightSecondaryColor = $ui_lightBlack;

/*--------------
  Page Heading
---------------*/

$ui_headerFontWeight = bold;
$ui_headerLineHeight = 1.2857em;

$ui_h1 = 2rem;
$ui_h2 = 1.714rem;
$ui_h3 = 1.28rem;
$ui_h4 = 1.071rem;
$ui_h5 = 1rem;

/*--------------
   Form Input
---------------*/

/* This adjusts the default form input across all elements */
$ui_inputBackground        = $ui_white;
$ui_inputVerticalPadding   = $ui_relative11px;
$ui_inputHorizontalPadding = $ui_relative14px;
$ui_inputPadding           = $ui_inputVerticalPadding $ui_inputHorizontalPadding;

/* Input Text Color */
$ui_inputColor= $ui_textColor;
$ui_inputPlaceholderColor= lighten($ui_inputColor, 75);
$ui_inputPlaceholderFocusColor= lighten($ui_inputColor, 45);

/* Line Height Default For Inputs in Browser */
$ui_inputLineHeight= 1.2142em;

/*-------------------
    Focused Input
--------------------*/

/* Used on inputs, textarea etc */
$ui_focusedFormBorderColor= #85B7D9;

/* Used on dropdowns, other larger blocks */
$ui_focusedFormMutedBorderColor= #96C8DA;

/*-------------------
        Sizes
--------------------*/

/*
  Sizes are all expressed in terms of 14px/em (default em)
  This ensures these "ratios" remain constant despite changes in EM
*/

$ui_miniSize        = (11 / 14);
$ui_tinySize        = (12 / 14);
$ui_smallSize       = (13 / 14);
$ui_mediumSize      = (14 / 14);
$ui_largeSize       = (16 / 14);
$ui_bigSize         = (18 / 14);
$ui_hugeSize        = (20 / 14);
$ui_massiveSize     = (24 / 14);


/*-------------------
        Page
--------------------*/

$ui_pageBackground      = #FFFFFF;
$ui_pageOverflowX       = hidden;

$ui_lineHeight          = 1.4285em;
$ui_textColor           = rgba(0, 0, 0, 0.87);

/*-------------------
      Paragraph
--------------------*/

$ui_paragraphMargin     = 0em 0em 1em;
$ui_paragraphLineHeight = $ui_lineHeight;

/*-------------------
       Links
--------------------*/

$ui_linkColor           = #4183C4;
$ui_linkUnderline       = none;
$ui_linkHoverColor      = darken(saturate($ui_linkColor, 20), 15, relative);
$ui_linkHoverUnderline  = $ui_linkUnderline;

/*-------------------
  Highlighted Text
--------------------*/

$ui_highlightBackground      = #CCE2FF;
$ui_highlightColor           = $ui_textColor;

$ui_inputHighlightBackground = rgba(100, 100, 100, 0.4);
$ui_inputHighlightColor      = $ui_textColor;


/*-------------------
       Loader
--------------------*/

$ui_loaderSize              = $ui_relativeBig;
$ui_loaderSpeed             = 0.6s;
$ui_loaderLineWidth         = 0.2em;
$ui_loaderFillColor         = rgba(0, 0, 0, 0.1);
