$ui_invertedLoaderFillColor = rgba(255, 255, 255, 0.15);
$ui_invertedLoaderLineColor = $ui_white;

/*-------------------
        Grid
--------------------*/

$ui_columnCount= 16;

/*-------------------
     Transitions
--------------------*/

$ui_defaultDuration = 0.1s;
$ui_defaultEasing   = ease;

/*-------------------
